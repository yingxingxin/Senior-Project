{
  "name": "senior-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:migrate-quizzes": "node scripts/migrate-quizzes.js",
    "db:migrate-quiz-progress": "node scripts/migrate-quiz-progress.js",
    "worker": "tsx --tsconfig tsconfig.worker.json scripts/worker.ts",
    "worker:dev": "tsx watch --tsconfig tsconfig.worker.json scripts/worker.ts",
    "test:agent": "npx tsx src/lib/ai/agent/__tests__/agent-mock.test.ts",
    "docker:worker:build": "docker build -f Dockerfile.worker -t senior-project-worker:latest .",
    "docker:worker:up": "docker compose -f docker-compose.worker.yml up -d",
    "docker:worker:down": "docker compose -f docker-compose.worker.yml down",
    "docker:worker:logs": "docker compose -f docker-compose.worker.yml logs -f worker",
    "docker:worker:deploy": "docker compose -f docker-compose.worker.yml up -d --build --force-recreate",
    "remotion:studio": "remotion studio remotion/index.tsx --config=remotion/remotion.config.ts",
    "remotion:render": "remotion render remotion/index.tsx DemoVideo out/demo.mp4 --config=remotion/remotion.config.ts",
    "remotion:render:preview": "remotion render remotion/index.tsx DemoVideo out/demo-preview.mp4 --config=remotion/remotion.config.ts --quality=50"
  },
